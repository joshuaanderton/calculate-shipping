{
  "name": "joshuaanderton/shipping",
  "description": "Shipping calculator for Laravel + Inertia.js",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.1|^8.2"
  },
  "require-dev": {
      "orchestra/testbench": "^8.0",
      "pestphp/pest-plugin-laravel": "^2.3",
      "pestphp/pest-plugin-watch": "^2.1"
  },
  "autoload": {
      "psr-4": {
          "Ja\\Shipping\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Ja\\Shipping\\Tests\\": "tests"
      }
  },
  "scripts": {
      "test": "vendor/bin/pest tests",
      "test-coverage": "vendor/bin/pest tests --coverage"
  },
  "config": {
      "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Ja\\Shipping\\Providers\\ServiceProvider"
          ]
      }
  }
}
